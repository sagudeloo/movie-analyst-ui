{
  "name": "movie-analyst-ui",
  "version": "1.0.0",
  "description": "ui for the movie analyst website....im totally not ripping this off.......",
  "scripts": {
    "start": "node server.js",
    "ui": "pm2 --name ui start npm -- start",
    "setuptest": "npm run ui && nohup json-server db.json >> ./json-server.log 2>&1 </dev/null &",
    "postsetuptest": "sleep 5",
    "test": "testcafe chrome:headless tests/test.js",
    "pretest": "npm run setuptest"
  },
