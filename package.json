  "bugs": {
    "url": "https://github.com/juan-ruiz/movie-analyst-ui/issues"
  },
  "homepage": "https://github.com/juan-ruiz/movie-analyst-ui#readme",
  "dependencies": {
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "json-server": "^0.16.3",
    "pm2": "^5.1.0",
    "testcafe": "^1.15.1"
  }
}
